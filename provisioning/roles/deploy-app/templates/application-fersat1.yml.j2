logging:
  level:
    root: ERROR

shell:
  out:
    info: CYAN
    success: GREEN
    warning: YELLOW
    error: RED

rotatorshell:
  rotctldHost: localhost # rotctld host
  rotctldPort: 4533 # rotctld port
  setPosWaitStep: 200 # milliseconds
  setPosWaitNumSteps: 3
  setPosWaitTimeout: 120 # seconds

